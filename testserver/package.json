{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.43",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-konva": "^18.2.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.7.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "npm run mkdir_lib && npm run link_lib && npm run link_react",
    "mkdir_lib": "cd node_modules && rm -rf @fendy3002/qz-react-evaluator && mkdir -p @fendy3002",
    "link_lib": "cd node_modules/@fendy3002 && ln -s ../../../qz-react-evaluator ./qz-react-evaluator",
    "link_react": "cd node_modules && rm -rf react react-dom && ln -s ../../qz-react-evaluator/node_modules/react ./react && ln -s ../../qz-react-evaluator/node_modules/react-dom ./react-dom"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
